{
  "name": "osis_document",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build --target lib --name osis-document --entry frontend/main.js --mode production",
    "coverage": "jest --coverage",
    "lint": "vue-cli-service lint frontend",
    "storybook": "vue-cli-service storybook:serve -p 6006 -c frontend/.storybook",
    "test": "jest",
    "watch": "vue-cli-service build --target lib --name osis-document --entry frontend/main.js --mode production --watch"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addon-storyshots": "^6.2.9",
    "@storybook/vue": "^6.0.26",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.2.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.9.0",
    "fetch-mock": "^9.11.0",
    "jest": "^26.6.3",
    "lint-staged": "^9.5.0",
    "mock-xmlhttprequest": "^7.0.4",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11",
    "vue-cli-plugin-storybook": "~2.0.0",
    "vue-i18n": "^8.24.4",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-i18n": "^8.24.4"
  },
  "babel": {
    "presets": [
      "@vue/cli-plugin-babel/preset"
    ]
  },
  "browserslist": [
    "defaults",
    "ie 11"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      ".*\\.(js)$": "babel-jest"
    },
    "collectCoverageFrom": [
      "frontend/**/*.{js,vue}",
      "!frontend/**/*.stories.js",
      "!frontend/main.js"
    ]
  }
}
